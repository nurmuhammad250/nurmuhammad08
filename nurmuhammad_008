import random
son = []
hariflar =['b','d','t','q','y','u']
sonlar =['0','1','2','3','4','5','6']
katta_hariflar=['A','B','D','E','F','G','U']

def parol():
    for i in range(random.randint(2,5)):
        son.append(random.choice(hariflar))
    for i in range(random.randint(3,5)):
        son.append(random.choice(sonlar))
    for i in range(random.randint(2,4)):
        son.append(random.choice(katta_hariflar))
    
    random.shufle(son)
    return son
